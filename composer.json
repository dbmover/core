{
    "name": "dbmover/core",
    "description": "PHP-based database versioning tool, core components",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=7.3",
        "monomelodies/kingconf": "^1.1"
    },
    "require-dev": {
        "gentry/gentry": "^0.16.11",
        "toast/unit": "^2.2.1",
        "gentry/toast": "^0.1.0"
    },
    "bin": [
        "bin/dbmover"
    ],
    "autoload": {
        "psr-4": {
            "Dbmover\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbmover\\Test\\": "tests"
        }
    }
}
